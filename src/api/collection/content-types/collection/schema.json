{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "contract_address": {
      "type": "string",
      "required": true
    },
    "chain_id": {
      "type": "integer",
      "required": true
    },
    "chain_name": {
      "type": "string",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "feature_img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive"
      ],
      "required": true,
      "default": "Inactive"
    },
    "desc": {
      "type": "text"
    }
  }
